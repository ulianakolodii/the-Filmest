{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "include": ["env.d.ts", "src/**/*", "src/**/*.vue"],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    },
    "target": "es5",
    "lib": ["es2019", "dom"],
    "esModuleInterop": true,
    "ignoreDeprecations": "5.0"
  },

  "references": [
    {
      "path": "./tsconfig.node.json"
    }
  ]
}
